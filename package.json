{
  "name": "template-reactjs",
  "private": true,
  "version": "0.0.18",
  "type": "module",
  "scripts": {
    "build": "node ./config/pre-build.js --mode development",
    "dev": "vite --host --port=80",
    "eslint": "npx eslint . --fix",
    "eslint:check": "npx eslint .",
    "lint": "node ./config/pre-build.js --ignore-build --fix",
    "lint:check": "pnpm eslint:check && pnpm prettier:check",
    "lint:fix": "pnpm eslint && pnpm prettier",
    "prepare": "husky",
    "prettier": "prettier -w .",
    "prettier:check": "prettier -c .",
    "preview": "vite preview --host",
    "start": "pnpm run dev",
    "update": "ncu",
    "update:check": "ncu --interactive --format group"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "6.1.6",
    "@mui/lab": "6.0.0-beta.14",
    "@mui/material": "6.1.6",
    "@mui/x-date-pickers": "7.22.0",
    "dayjs": "1.11.13",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sweetalert2": "11.14.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-angular": "^19.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "globals": "^15.11.0",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.10",
    "prettier": "^3.3.3",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=20.6.1",
    "npm": "please-use-pnpm",
    "pnpm": ">9.0.0",
    "yarn": "please-use-pnpm"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier -w",
      "eslint . --fix"
    ],
    "*.{html,css,md,json}": [
      "prettier -w"
    ]
  }
}
